{
  "manifest_version": 2,

  "name": "WebToonAlarm",
  "description": "It informes you new webtoon",
  "version": "1.0",
  
  "browser_action": {
  	"name" : "first chrome Ex",
  	"icons": ["icon.png"],
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "https://secure.flickr.com/",
    "http://comic.naver.com/webtoon/*",
    "tabs",
    "bookmarks",
    "http://*/*",
    "https://*/*",
    "http://jquery.com/*"
  ]
}
